@model GamesGameInfoViewModel


<h1>Game Info</h1>
<div class="card mx-auto" style="width: 18rem;">
    <partial name="_gameCardPartial" model="@Model" />
    <div class="card-footer">
        <partial name="_ratingStarPartial" model="@Model.Rating" />
    </div>
    <a asp-controller="Games"
       asp-action="GetGames" class="btn btn-primary w-25 m-1">Back</a>
</div>



